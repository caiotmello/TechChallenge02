    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>abd9111d-99fa-43c9-82b2-2cd69394b539</UserSecretsId>
    <DockerDefaultTargetOS>Windows</DockerDefaultTargetOS>
  </PropertyGroup>

  <ItemGroup>
